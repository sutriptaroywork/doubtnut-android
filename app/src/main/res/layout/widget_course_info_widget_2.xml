<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="17dp"
    android:layout_marginEnd="16dp">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_one"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.33" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_two"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.66" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/tv_title_one"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lato_bold"
        android:includeFontPadding="false"
        android:maxWidth="100dp"
        android:textAllCaps="false"
        android:textColor="@color/black"
        app:layout_constraintEnd_toStartOf="@id/iv_title_one"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="7 Jun-30 Dec" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/iv_title_one"
        android:layout_width="14dp"
        android:layout_height="14dp"
        app:layout_constraintBottom_toBottomOf="@id/tv_title_one"
        app:layout_constraintStart_toEndOf="@id/tv_title_one"
        app:srcCompat="@drawable/ic_arrow_down_24px"
        tools:visibility="gone" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/tv_subtitle_one"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/lato"
        android:includeFontPadding="false"
        android:textColor="@color/grey_808080"
        android:textSize="12sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline_one"
        app:layout_constraintStart_toStartOf="@+id/tv_title_one"
        app:layout_constraintTop_toBottomOf="@+id/tv_title_one"
        tools:text="Course Duration" />

    <View
        android:id="@+id/view_divider_one"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:background="@color/grey_cbcbcb"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/guideline_one"
        app:layout_constraintStart_toStartOf="@id/guideline_one"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible" />

    <View
        android:id="@+id/view_click_one"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/guideline_one"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="UnusedAttribute" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/tv_title_two"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:fontFamily="@font/lato_bold"
        android:includeFontPadding="false"
        android:maxWidth="100dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toStartOf="@id/iv_title_two"
        app:layout_constraintStart_toEndOf="@id/guideline_one"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="English" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/iv_title_two"
        android:layout_width="14dp"
        android:layout_height="14dp"
        app:layout_constraintBottom_toBottomOf="@id/tv_title_two"
        app:layout_constraintStart_toEndOf="@id/tv_title_two"
        app:srcCompat="@drawable/ic_arrow_down_24px"
        tools:visibility="gone" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/tv_subtitle_two"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/lato"
        android:includeFontPadding="false"
        android:textColor="@color/grey_808080"
        android:textSize="12sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline_two"
        app:layout_constraintStart_toStartOf="@+id/tv_title_two"
        app:layout_constraintTop_toBottomOf="@+id/tv_title_two"
        tools:text="Study Medium" />

    <View
        android:id="@+id/view_click_two"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/guideline_two"
        app:layout_constraintStart_toEndOf="@id/guideline_one"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="UnusedAttribute" />

    <View
        android:id="@+id/view_divider_two"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:background="@color/grey_cbcbcb"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/guideline_two"
        app:layout_constraintStart_toStartOf="@id/guideline_two"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/tv_title_three"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:fontFamily="@font/lato_bold"
        android:includeFontPadding="false"
        android:maxWidth="100dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toStartOf="@id/iv_title_three"
        app:layout_constraintStart_toEndOf="@id/guideline_two"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="4.2K" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/iv_title_three"
        android:layout_width="14dp"
        android:layout_height="14dp"
        app:layout_constraintBottom_toBottomOf="@id/tv_title_three"
        app:layout_constraintStart_toEndOf="@id/tv_title_three"
        app:srcCompat="@drawable/ic_arrow_down_24px"
        tools:visibility="gone" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/tv_subtitle_three"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lato"
        android:includeFontPadding="false"
        android:textColor="@color/grey_808080"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/tv_title_three"
        app:layout_constraintTop_toBottomOf="@id/tv_title_three"
        tools:text="Enrolled Students" />

    <View
        android:id="@+id/view_click_three"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/guideline_two"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="UnusedAttribute" />

</androidx.constraintlayout.widget.ConstraintLayout>

